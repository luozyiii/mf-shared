{
  "name": "mf-shared",
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.13.1",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rsbuild build",
    "build:storybook": "storybook build",
    "dev": "rsbuild dev",
    "storybook": "storybook dev -p 6006",
    "lint": "biome lint src/",
    "lint:fix": "biome check --write src/",
    "format": "biome format --write src/",
    "format:check": "biome format src/",
    "type-check": "tsc --noEmit",
    "code-quality": "pnpm run lint && pnpm run format:check && pnpm run type-check",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "biome check --write"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@module-federation/rsbuild-plugin": "^0.18.3",
    "@module-federation/storybook-addon": "^4.0.27",
    "@rsbuild/core": "^1.4.15",
    "@rsbuild/plugin-react": "^1.3.5",
    "@rslib/core": "^0.12.2",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@types/node": "^22.17.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "~19.1.7",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "storybook": "^8.6.14",
    "storybook-addon-rslib": "^1.0.3",
    "storybook-react-rsbuild": "^1.0.3",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  }
}
